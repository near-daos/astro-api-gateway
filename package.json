{
  "name": "sputnik-v2-api",
  "version": "0.0.3",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build-api": "nest build api",
    "build-aggregator": "nest build aggregator",
    "build-notifier": "nest build notifier",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start-api": "nest start api",
    "start-api:dev": "nest start api --watch",
    "start-aggregator": "nest start aggregator",
    "start-aggregator:dev": "nest start aggregator --watch",
    "start-notifier": "nest start notifier",
    "start-notifier:dev": "nest start notifier --watch",
    "start": "concurrently \"yarn start-aggregator\" \"yarn start-api\" \"yarn start-notifier\"",
    "start:dev": "concurrently \"yarn start-aggregator:dev\" \"yarn start-api:dev\" \"yarn start-notifier:dev\"",
    "start-migrations": "nest start migrations",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test-api:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "test-aggregator:e2e": "jest --config ./apps/aggregator/test/jest-e2e.json",
    "test-notifier:e2e": "jest --config ./apps/notifier/test/jest-e2e.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs/axios": "^0.0.3",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/microservices": "^7.6.18",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/platform-socket.io": "^7.6.18",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/throttler": "^2.0.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjs/websockets": "^7.6.18",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-request": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@supercharge/promise-pool": "^1.7.0",
    "@types/cache-manager": "^3.4.2",
    "amqp-connection-manager": "^3.2.2",
    "amqplib": "^0.8.0",
    "aws-sdk": "^2.937.0",
    "cache-manager": "^3.4.4",
    "cache-manager-redis-store": "^2.0.0",
    "camelcase-keys": "^7.0.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "firebase-admin": "^9.10.0",
    "ioredis": "^4.14.1",
    "near-api-js": "^0.44.0",
    "object-hash": "^2.2.0",
    "parse-redis-url-simple": "^1.0.2",
    "pg": "^8.6.0",
    "prom-client": "^14.0.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "socket.io": "^4.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "typeorm-naming-strategies": "^2.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/express": "^4.17.11",
    "@types/hapi__joi": "^17.1.6",
    "@types/ioredis": "^4.27.4",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "sputnik-v2/config/(.*)": "<rootDir>/libs/config/src/$1",
      "sputnik-v2/config": "<rootDir>/libs/config/src",
      "sputnik-v2/common/(.*)": "<rootDir>/libs/common/src/$1",
      "sputnik-v2/common": "<rootDir>/libs/common/src",
      "@sputnik-v2/subscription/(.*)": "<rootDir>/libs/subscription/src/$1",
      "@sputnik-v2/subscription": "<rootDir>/libs/subscription/src",
      "@sputnik-v2/dao/(.*)": "<rootDir>/libs/dao/src/$1",
      "@sputnik-v2/dao": "<rootDir>/libs/dao/src",
      "@sputnik-v2/account/(.*)": "<rootDir>/libs/account/src/$1",
      "@sputnik-v2/account": "<rootDir>/libs/account/src",
      "@sputnik-v2/near-indexer/(.*)": "<rootDir>/libs/near-indexer/src/$1",
      "@sputnik-v2/near-indexer": "<rootDir>/libs/near-indexer/src",
      "@sputnik-v2/bounty/(.*)": "<rootDir>/libs/bounty/src/$1",
      "@sputnik-v2/bounty": "<rootDir>/libs/bounty/src",
      "@sputnik-v2/event/(.*)": "<rootDir>/libs/event/src/$1",
      "@sputnik-v2/event": "<rootDir>/libs/event/src",
      "@sputnik-v2/near-api/(.*)": "<rootDir>/libs/near-api/src/$1",
      "@sputnik-v2/near-api": "<rootDir>/libs/near-api/src",
      "@sputnik-v2/utils/(.*)": "<rootDir>/libs/utils/src/$1",
      "@sputnik-v2/utils": "<rootDir>/libs/utils/src",
      "@sputnik-v2/proposal/(.*)": "<rootDir>/libs/proposal/src/$1",
      "@sputnik-v2/proposal": "<rootDir>/libs/proposal/src",
      "@sputnik-v2/sputnikdao/(.*)": "<rootDir>/libs/sputnikdao/src/$1",
      "@sputnik-v2/sputnikdao": "<rootDir>/libs/sputnikdao/src",
      "@sputnik-v2/token/(.*)": "<rootDir>/libs/token/src/$1",
      "@sputnik-v2/token": "<rootDir>/libs/token/src",
      "@sputnik-v2/transaction/(.*)": "<rootDir>/libs/transaction/src/$1",
      "@sputnik-v2/transaction": "<rootDir>/libs/transaction/src",
      "@sputnik-v2/cache/(.*)": "<rootDir>/libs/cache/src/$1",
      "@sputnik-v2/cache": "<rootDir>/libs/cache/src",
      "@sputnik-v2/transaction-handler/(.*)": "<rootDir>/libs/transaction-handler/src/$1",
      "@sputnik-v2/transaction-handler": "<rootDir>/libs/transaction-handler/src",
      "@sputnik-v2/migrations/(.*)": "<rootDir>/libs/migrations/src/$1",
      "@sputnik-v2/migrations": "<rootDir>/libs/migrations/src",
      "@sputnik-v2/notification/(.*)": "<rootDir>/libs/notification/src/$1",
      "@sputnik-v2/notification": "<rootDir>/libs/notification/src",
      "@sputnik-v2/comment/(.*)": "<rootDir>/libs/comment/src/$1",
      "@sputnik-v2/comment": "<rootDir>/libs/comment/src"
    }
  }
}
